{% set cards = [
    {
        img: 'images/content/catalog/card1.png',
        title: 'Вахтовые автобусы'
    },
    {
        img: 'images/content/catalog/card2.png',
        title: 'Спецавтомобили МЧС'
    },
    {
        img: 'images/content/catalog/card3.png',
        title: 'Передвижные лаборатории'
    },
    {
        img: 'images/content/catalog/card4.png',
        title: 'Передвижные мастерские'
    },
    {
        img: 'images/content/catalog/card5.png',
        title: 'Жилые модули'
    },
    {
        img: 'images/content/catalog/card6.png',
        title: 'Спецавтомобили МВД'
    },
    {
        img: 'images/content/catalog/card7.png',
        title: 'Цельнометаллические фургоны'
    },
    {
        img: 'images/content/catalog/card8.png',
        title: 'Изотермические фургоны'
    }
] %}

<div class="catalog-list">
        {% for item in cards %}
        <div class="catalog-card">
            <a href="#" class="catalog-card-link">
                <span class="catalog-card__img"  style="background-image: url('{{item.img}}')"></span>
                <span class="catalog-card__title">{{ item.title }}</span>
            </a>

        </div>
        {% endfor %}

</div>
